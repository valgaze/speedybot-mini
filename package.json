{
  "name": "speedybot-mini",
  "version": "2.0.2",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "jest": "^29.2.0",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsx": "^3.10.1",
    "typedoc": "0.22.18",
    "typedoc-plugin-markdown": "3.12.1",
    "typedoc-theme-hierarchy": "1.2.1",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "save": "git add . &2.0.0-beta.& git commit -m \"checkpt\" && git push draft deploy",
    "publish:beta": "npm run build && npm publish --tag beta",
    "docs": "rm -rf api-docs 2>/dev/null && npx --yes typedoc --media docs/assets --plugin typedoc-plugin-markdown --out api-docs src/docs.ts",
    "build": "npm test && tsc && npm run docs",
    "test": "jest",
    "dev": "tsnd --respawn --watch src/lib, src/lib/index"
  },
  "engines": {
    "node": ">=18.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valgaze/speedybot.git"
  },
  "keywords": [
    "bot",
    "scaffold",
    "continerless",
    "speedybot",
    "serverless"
  ],
  "author": "valgaze@gmail.com",
  "license": "MIT"
}
